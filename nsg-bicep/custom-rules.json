{
  "securityRules": [
    {
      "name": "Allow_Internet_HTTP_Inbound",
      "properties": {
        "description": "Allow inbound internet connectivity for HTTP only.",
        "protocol": "Tcp",
        "sourceAddressPrefix": "Internet",
        "sourcePortRange": "*",
        "destinationAddressPrefix": "VirtualNetwork",
        "destinationPortRange": "80",
        "access": "Allow",
        "priority": 500,
        "direction": "Inbound"
      }
    }
  ]
}